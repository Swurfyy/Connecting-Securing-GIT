! NY Router-on-a-Stick 1
enable
configure terminal
hostname NY-ROAS1
ip domain-name allsafe1.edu
no ip domain-lookup
! SSH
crypto key generate rsa
1024
ip ssh version 2
! Trunk naar ACSW1
interface FastEthernet0/1
 description Trunk to NY-ACSW1 Fa0/1
 no ip address
 no shutdown
interface FastEthernet0/1.10
 description VLAN10 DATA
 encapsulation dot1Q 10
 ip address 10.1.4.1 255.255.255.0
 no shutdown
interface FastEthernet0/1.20
 description VLAN20 WIRELESS
 encapsulation dot1Q 20
 ip address 10.1.0.1 255.255.252.0
 no shutdown
interface FastEthernet0/1.100
 description VLAN100 MGMT
 encapsulation dot1Q 100
 ip address 10.1.5.1 255.255.255.192
 no shutdown
interface FastEthernet0/1.200
 description VLAN200 SERVERS
 encapsulation dot1Q 200
 ip address 10.1.6.1 255.255.255.0
 no shutdown
! Link naar FW_NY inside
interface FastEthernet0/0
 description Link to FW_NY inside
 ip address 10.1.5.242 255.255.255.192
 no shutdown
! Interconnect naar ROAS2
interface Serial0/1/0
 description P2P link to NY-ROAS2
 ip address 172.16.0.1 255.255.255.252
 no shutdown
! Routing
ip route 0.0.0.0 0.0.0.0 10.1.5.241
router ospf 1
 router-id 10.1.5.242
 network 10.1.0.0 0.0.3.255 area 0
 network 10.1.4.0 0.0.0.255 area 0
 network 10.1.5.0 0.0.0.63 area 0
 network 10.1.6.0 0.0.0.255 area 0
 network 10.1.5.240 0.0.0.3 area 0
 network 172.16.0.0 0.0.0.3 area 0
 default-information originate
 passive-interface FastEthernet0/1
! Management & lines
line vty 0 4
 transport input ssh
 logging synchronous
line con 0
 logging synchronous
end

