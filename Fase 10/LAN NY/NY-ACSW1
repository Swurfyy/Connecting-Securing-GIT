! Access Switch 1 New York
enable
configure terminal
hostname NY-ACSW1
no ip domain-lookup
! VLANs
vlan 10
 name DATA
vlan 20
 name WIRELESS
vlan 100
 name MGMT
vlan 200
 name SERVERS
! Trunk naar ROAS1
interface FastEthernet0/1
 description Trunk to NY-ROAS1
 switchport mode trunk
 switchport trunk allowed vlan 10,20,100,200
! Mgmt SVI
interface Vlan100
 ip address 10.1.5.3 255.255.255.192
 no shutdown
ip default-gateway 10.1.5.1
! Lines
line vty 0 4
 logging synchronous
line con 0
 logging synchronous
end

