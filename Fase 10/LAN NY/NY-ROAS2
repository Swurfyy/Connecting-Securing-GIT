! NY Router-on-a-Stick 2 (simpel)
enable
configure terminal
hostname NY-ROAS2
no ip domain-lookup
! Trunk naar ACSW2
interface FastEthernet0/1
 description Trunk to NY-ACSW2 Fa0/1
 no ip address
 no shutdown
interface FastEthernet0/1.10
 description VLAN10 DATA
 encapsulation dot1Q 10
 ip address 10.1.4.2 255.255.255.0
 no shutdown
interface FastEthernet0/1.20
 description VLAN20 WIRELESS
 encapsulation dot1Q 20
 ip address 10.1.0.2 255.255.252.0
 no shutdown
interface FastEthernet0/1.100
 description VLAN100 MGMT
 encapsulation dot1Q 100
 ip address 10.1.5.2 255.255.255.192
 no shutdown
interface FastEthernet0/1.200
 description VLAN200 SERVERS
 encapsulation dot1Q 200
 ip address 10.1.6.2 255.255.255.0
 no shutdown
! Link naar FW_NY inside
interface FastEthernet0/0
 description Link to FW_NY inside
 ip address 10.1.5.243 255.255.255.192
 no shutdown
! P2P naar ROAS1
interface Serial0/1/1
 description P2P link to NY-ROAS1
 ip address 172.16.0.2 255.255.255.252
 no shutdown
! Simpele routing via ROAS1
ip route 0.0.0.0 0.0.0.0 172.16.0.1
! Lines
line vty 0 4
 transport input ssh
 logging synchronous
line con 0
 logging synchronous
end

